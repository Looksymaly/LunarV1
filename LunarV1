coroutine.wrap(CXQURDM_fake_script)()
local function XWYGB_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	-- Varibles --
	
	local Gui = script.Parent
	local ClearBtn = Gui.Clear
	local RunBtn = Gui.Run
	local HideBtn = Gui.Hide
	local CloseBtn = Gui.Close
	local InjectBtn = Gui.Inject
	local CommandTxtBox = Gui.CommandEditor
	local TweenService = game:GetService("TweenService")
	local UserInputService = game:GetService("UserInputService")
	local player = game.Players.LocalPlayer
	
	-- Inject propities --
	
	local isinjected = false
	
	-- Commands Functions --
	
	local function walkspeedset(WalkNum)
		if WalkNum then
			player.Character.Humanoid.WalkSpeed = WalkNum
		end
	end
	
	local function jumppowerset(JumpNum)
		if JumpNum then
			player.Character.Humanoid.WalkSpeed = JumpNum
		end
	end
	
	-- Functions --
	
	local function Clear()
		CommandTxtBox.Text = ""
	end
	
	local function Inject()
		if isinjected then
			warn("Its already injected")
		else
			isinjected = true
		end
	end
	
	-- Commands Functions --
	
	local function walkspeedset(WalkNum)
		if WalkNum then
			player.Character.Humanoid.WalkSpeed = WalkNum
		end
	end
	
	local function jumppowerset(JumpNum)
		if JumpNum then
			player.Character.Humanoid.JumpPower = JumpNum  -- Corrected to set JumpPower instead of WalkSpeed
		end
	end
	
	-- Run Function --
	
	local function Run()
		if isinjected then
			local command = CommandTxtBox.Text:lower()
	
			if command == "/playallsounds" then
				for _, sounds in pairs(game:GetDescendants()) do
					if sounds:IsA("Sound") then
						sounds.Looped = false
						sounds:Play()
					end
				end
			elseif command:match("^/walkspeed%s*(%d+)$") then
				local WalkNum = tonumber(command:match("^/walkspeed%s*(%d+)$"))
				walkspeedset(WalkNum)
			elseif command:match("^/jumppower%s*(%d+)$") then
				local JumpNum = tonumber(command:match("^/jumppower%s*(%d+)$"))
				jumppowerset(JumpNum)
			end
	
			-- Discord Command
			if command == "/discord" then
				local loading = script.Parent.Parent.Loading
				local main = script.Parent.Parent.Main
				local Notif = Instance.new("Sound")
				Notif.SoundId = "rbxassetid://17208372272"
				Notif.Parent = loading
				wait(2)
				-- Variables
				local part = script.Parent.Parent.Notif
				part.Des.Text = "Copied Discord link to clipboard. After go verify on our verification. Then you can chat!"
				local targetPosition = UDim2.new(0.353, 0, 0.015, 0)
				local tweenTime = 2
				local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out, 0, false)
				local goal = {Position = targetPosition}
				local tween = TweenService:Create(part, tweenInfo, goal)
				tween:Play()
				tween.Completed:Wait()
				Notif:Play()
				wait(5)
				local part = script.Parent.Notif
				local targetPosition = UDim2.new(0.353, 0, -1.2, 0)
				local tweenTime = 2
				local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Exponential, Enum.EasingDirection.InOut, 0, false)
				local goal = {Position = targetPosition}
				local tween = TweenService:Create(part, tweenInfo, goal)
				tween:Play()
			end
	
			-- Website Command
			if command == "/website" then
				local loading = script.Parent.Parent.Loading
				local main = script.Parent.Parent.Main
				local Notif = Instance.new("Sound")
				Notif.SoundId = "rbxassetid://17208372272"
				Notif.Parent = loading
				wait(2)
				-- Variables
				local part = script.Parent.Parent.Notif
				part.Des.Text = "Copied Website link to clipboard. You can find scripts and get our Source code!"
				local targetPosition = UDim2.new(0.353, 0, 0.015, 0)
				local tweenTime = 2
				local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out, 0, false)
				local goal = {Position = targetPosition}
				local tween = TweenService:Create(part, tweenInfo, goal)
				tween:Play()
				tween.Completed:Wait()
				Notif:Play()
				wait(5)
				local part = script.Parent.Notif
				local targetPosition = UDim2.new(0.353, 0, -1.2, 0)
				local tweenTime = 2
				local tweenInfo = TweenInfo.new(tweenTime, Enum.EasingStyle.Exponential, Enum.EasingDirection.InOut, 0, false)
				local goal = {Position = targetPosition}
				local tween = TweenService:Create(part, tweenInfo, goal)
				tween:Play()
			end
	
			-- Kick Command
			if command:match("^/kick%s*(random|all|others|%w+)$") then
				local target = command:match("^/kick%s*(%w+)$") or command:match("^/kick%s*(random|all|others)$")
	
				if target == "random" then
					local players = game.Players:GetPlayers()
					local randomPlayer = players[math.random(1, #players)]
					randomPlayer:Kick("You have been kicked!")
				elseif target == "all" then
					for _, player in ipairs(game.Players:GetPlayers()) do
						player:Kick("You have been kicked!")
					end
				elseif target == "others" then
					for _, player in ipairs(game.Players:GetPlayers()) do
						if player ~= game.Players.LocalPlayer then
							player:Kick("You have been kicked!")
						end
					end
				else
					local playerToKick = game.Players:FindFirstChild(target)
					if playerToKick then
						playerToKick:Kick("You have been kicked!")
					else
						warn("Player not found: " .. target)
					end
				end
			end
	
			-- Freeze Command
			if command:match("^/freeze%s*(random|all|others|%w+)$") then
				local target = command:match("^/freeze%s*(%w+)$") or command:match("^/freeze%s*(random|all|others)$")
	
				if target == "random" then
					local players = game.Players:GetPlayers()
					local randomPlayer = players[math.random(1, #players)]
					randomPlayer.Character.HumanoidRootPart.Anchored = true
				elseif target == "all" then
					for _, player in ipairs(game.Players:GetPlayers()) do
						player.Character.HumanoidRootPart.Anchored = true
					end
				elseif target == "others" then
					for _, player in ipairs(game.Players:GetPlayers()) do
						if player ~= game.Players.LocalPlayer then
							player.Character.HumanoidRootPart.Anchored = true
						end
					end
				else
					local playerToFreeze = game.Players:FindFirstChild(target)
					if playerToFreeze then
						playerToFreeze.Character.HumanoidRootPart.Anchored = true
					else
						warn("Player not found: " .. target)
					end
				end
			end
	
			-- Kill Command
			if command:match("^/kill%s*(random|all|others|%w+)$") then
				local target = command:match("^/kill%s*(%w+)$") or command:match("^/kill%s*(random|all|others)$")
	
				if target == "random" then
					local players = game.Players:GetPlayers()
					local randomPlayer = players[math.random(1, #players)]
					randomPlayer.Character.Humanoid.Health = 0
				elseif target == "all" then
					for _, player in ipairs(game.Players:GetPlayers()) do
						player.Character.Humanoid.Health = 0
					end
				elseif target == "others" then
					for _, player in ipairs(game.Players:GetPlayers()) do
						if player ~= game.Players.LocalPlayer then
							player.Character.Humanoid.Health = 0
						end
					end
				else
					local playerToKill = game.Players:FindFirstChild(target)
					if playerToKill then
						playerToKill.Character.Humanoid.Health = 0
					else
						warn("Player not found: " .. target)
					end
				end
			end
		else
			warn("Must be injected")
		end
	end
	
	
	local function Hide()
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("Frame") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {Transparency = 1}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play()
				tween.Completed:Wait() 
			end
		end
	
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("TextLabel") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {TextTransparency = 1}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play() 
			end
		end
		wait(1.3)
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("ImageLabel") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {ImageTransparency = 1}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play()
				tween.Completed:Wait() 
			end
		end
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("ImageButton") or descendant:IsA("TextBox") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {Transparency = 1}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play() 
			end
		end
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("ImageButton") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {ImageTransparency = 1}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play() 
			end
		end
	
		wait(1.2)
		local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
		local tweenGoal = {Transparency = 1}
		local tween = TweenService:Create(Gui, tweenInfo, tweenGoal)
	
		tween:Play()
		tween.Completed:Wait() 
	
		Gui.Visible = false
	end
	
	local function Close()
		Hide()
		Gui.Parent:Destroy()
	end
	
	local function Show()
		Gui.Visible = true
	
		local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
		local tweenGoal = {Transparency = 0}
		local tween = TweenService:Create(Gui, tweenInfo, tweenGoal)
	
		tween:Play()
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("Frame") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {Transparency = 0}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play()
				tween.Completed:Wait() 
			end
		end
	
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("TextLabel") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {TextTransparency = 0}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play() 
			end
		end
		wait(1.3)
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("ImageLabel") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {ImageTransparency = 0}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play()
				tween.Completed:Wait() 
			end
		end
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("ImageButton") or descendant:IsA("TextBox") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {Transparency = 0}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play() 
			end
		end
	
		for _, descendant in pairs(script.Parent:GetDescendants()) do
			if descendant:IsA("ImageButton") then
				local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
				local tweenGoal = {ImageTransparency = 0}
				local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
				tween:Play() 
			end
		end
	end
	
	-- Run Functions --
	
	RunBtn.MouseButton1Click:Connect(Run)
	
	InjectBtn.MouseButton1Click:Connect(Inject)
	
	ClearBtn.MouseButton1Click:Connect(Clear)
	
	HideBtn.MouseButton1Click:Connect(Hide)
	
	CloseBtn.MouseButton1Click:Connect(Close)
	
	UserInputService.InputBegan:Connect(function(input)
		if input.KeyCode == Enum.KeyCode.LeftAlt then
			Show()
		end
	end)
end
coroutine.wrap(XWYGB_fake_script)()
local function OUWM_fake_script() -- Lunar.SoundHandler 
	local script = Instance.new('LocalScript', Lunar)

	script.Parent.Notif.Position = UDim2.new(0.353, 0,-1.2, 0)
	wait(1)
	print("Sounds has loaded")
	local loading = script.Parent.Loading
	local main = script.Parent.Main
	
	local Notif = Instance.new("Sound")
	Notif.SoundId = "rbxassetid://17208372272"
	Notif.Parent = loading
	wait(2)
	-- Variables
	local part = script.Parent.Notif  -- The part to tween
	local targetPosition = UDim2.new(0.353, 0,0.015, 0)  -- The target position
	local tweenTime = 2  -- Time to complete the tween
	
	-- TweenService
	local TweenService = game:GetService("TweenService")
	
	-- Tween Info
	local tweenInfo = TweenInfo.new(
		tweenTime,  -- Time for the tween to complete
		Enum.EasingStyle.Exponential,  -- Easing style for a smooth transition
		Enum.EasingDirection.Out,  -- Easing direction for a smooth end
		0,  -- Repeat infinitely
		false  -- Reverse the tween after completing
	)
	
	-- Goal (end position of the part)
	local goal = {
		Position = targetPosition
	}
	
	-- Create the tween
	local tween = TweenService:Create(part, tweenInfo, goal)
	
	-- Play the tween
	tween:Play()
	tween.Completed:Wait()
	Notif:Play()
	wait(5)
	
	local part = script.Parent.Notif  -- The part to tween
	local targetPosition = UDim2.new(0.353, 0,-1.2, 0)  -- The target position
	local tweenTime = 2  -- Time to complete the tween
	
	-- TweenService
	local TweenService = game:GetService("TweenService")
	
	-- Tween Info
	local tweenInfo = TweenInfo.new(
		tweenTime,  -- Time for the tween to complete
		Enum.EasingStyle.Exponential,  -- Easing style for a smooth transition
		Enum.EasingDirection.InOut,  -- Easing direction for a smooth end
		0,  -- Repeat infinitely
		false  -- Reverse the tween after completing
	)
	
	-- Goal (end position of the part)
	local goal = {
		Position = targetPosition
	}
	
	-- Create the tween
	local tween = TweenService:Create(part, tweenInfo, goal)
	tween:Play()
end
coroutine.wrap(OUWM_fake_script)()
