coroutine.wrap(OUNN_fake_script)()
local function UTRNIQ_fake_script() -- Loading.Handler 
	local script = Instance.new('LocalScript', Loading)

	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("Frame") or descendant:IsA("TextLabel") or descendant:IsA("TextButton") or descendant:IsA("ImageLabel") or descendant:IsA("TextBox") then
			descendant.Transparency = 1
		end
	end
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("ImageLabel") then
			descendant.ImageTransparency = 1
		end
	end
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("ImageButton") then
			descendant.BackgroundTransparency = 1
			descendant.ImageTransparency = 1
		end
	end
	
	local TweenService = game:GetService("TweenService")
	local player = game.Players.LocalPlayer
	local bar = script.Parent.Bar.Progress
	
	print("🌕 Lunar loading...")
	
	-- Tween function
	local function tweenBar(goalSize, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Exponential, Enum.EasingDirection.Out)
		local goal = {Size = UDim2.new(goalSize, 0, 1, 0)}
		local tween = TweenService:Create(bar, tweenInfo, goal)
		tween:Play()
		tween.Completed:Wait()
	end
	
	-- Step 1: Check Filtering Enabled (FE)
	wait(2)
	tweenBar(0.1, 1.3)
	print("Checking FE...")
	if not game.SoundService.RespectFilteringEnabled then
		print("🟢 Loaded FE")
	else
		warn("FE didnt get Loaded. Some things may be client sided.")
	end
	
	-- Step 2: Load Player
	wait(3)
	tweenBar(0.2, 1.3)
	if player then
		print("🟢 Loaded Player")
	else
		print("Player not recognized...")
	end
	
	-- Step 3: Inject
	wait(1.5)
	tweenBar(0.35, 1.3)
	print(player.Name .. " has been injected...")
	
	-- Step 4: Final Loading
	wait(0.6)
	tweenBar(0.85, 1.3)
	print("🌕 Lunar Loaded...")
	
	wait(2)
	tweenBar(1, 0.5)
	
	-- Ensure full size
	if bar.Size.X.Scale == 1 then
		bar.Size = UDim2.new(1, 0, 1, 0)
	end
	
	
	for _, descendant in pairs(script.Parent:GetDescendants()) do
		if descendant:IsA("Frame") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {Transparency = 1}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play()
			tween.Completed:Wait() 
		end
	end
	
	
	for _, descendant in pairs(script.Parent:GetDescendants()) do
		if descendant:IsA("TextLabel") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {TextTransparency = 1}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play() 
		end
	end
	wait(1.3)
	
	for _, descendant in pairs(script.Parent:GetDescendants()) do
		if descendant:IsA("ImageLabel") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {ImageTransparency = 1}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play()
			tween.Completed:Wait() 
		end
	end
	
	wait(0.5)
	script.Parent.Visible = false
	script.Parent.Parent.Main.Position = script.Parent.Position
	script.Parent.Parent.Main.Visible = true
	print("🌕 Lunar running...")
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("Frame") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {Transparency = 0}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play()
			tween.Completed:Wait() 
		end
	end
	
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("TextLabel") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {TextTransparency = 0}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play() 
		end
	end
	wait(1.3)
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("ImageLabel") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {ImageTransparency = 0}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play()
			tween.Completed:Wait() 
		end
	end
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("ImageButton") or descendant:IsA("TextBox") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {Transparency = 0}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play() 
		end
	end
	
	for _, descendant in pairs(script.Parent.Parent.Main:GetDescendants()) do
		if descendant:IsA("ImageButton") then
			local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out, 0, false, 0)
			local tweenGoal = {ImageTransparency = 0}
			local tween = TweenService:Create(descendant, tweenInfo, tweenGoal)
	
			tween:Play() 
		end
	end
end
